{
  "name": "msbd",
  "version": "1.0.0",
  "main": "bin/typescript/app.js",
  "type": "module",
  "scripts": {
    "prebuild": "rm -fr bin",
    "build": "tsc",
    "postbuild": "npm run copy",
    "copy": "npm run copyCredentials && npm run copyPublic && npm run copyPython",
    "copyCredentials": "cp ./src/typescript/auth/credentials.json ./bin/typescript/auth/credentials.json",
    "copyPublic": "cp -r ./src/public ./bin/",
    "copyPython": "cp -r ./src/python ./bin/",
    "start": "node . & python bin/python/app.py"
  },
  "keywords": [
    "ReVA"
  ],
  "author": "Fedyna K",
  "license": "ISC",
  "description": "MÃ©dia Sociaux et Big Data",
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.21.0",
    "google-auth-library": "^9.14.1",
    "googleapis": "^144.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "typescript": "^5.6.2"
  }
}
